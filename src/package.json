{
  "scripts": {
    "common": "cd SeoToolkit.Umbraco.Common/assets && npm run build --watch",
    "robotsTxt": "cd SeoToolkit.Umbraco.RobotsTxt/assets && npm run build --watch",
    "scriptManager": "cd SeoToolkit.Umbraco.ScriptManager/assets && npm run build --watch",
    "redirect": "cd SeoToolkit.Umbraco.Redirects/assets && npm run build --watch",
    "sitemap": "cd SeoToolkit.Umbraco.Sitemap/assets && npm run build --watch",
    "siteAudit": "cd SeoToolkit.Umbraco.SiteAudit/assets && npm run build --watch",
    "start": "concurrently \"npm run common\" \"npm run robotsTxt\" \"npm run scriptManager\" \"npm run redirect\" \"npm run sitemap\" \"npm run siteAudit\""
  },
  "dependencies": {
    "concurrently": "^9.0.1"
  }
}